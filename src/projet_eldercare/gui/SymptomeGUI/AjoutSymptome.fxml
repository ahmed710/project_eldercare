<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600" prefWidth="1100" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projet_eldercare.gui.SymptomeGUI.AjoutSymptomeController">
    <stylesheets>
        <URL value="@AjoutSymptome.css" />
    </stylesheets>
    <children>
        <Label layoutX="129.0" layoutY="50.0" text="Ajout Symptomes" textFill="#ace2f5">
         <font>
            <Font name="System Bold" size="17.0" />
         </font></Label>
        
        <Label fx:id="titreLabel" layoutX="93.0" layoutY="127.0" text="Titre :" textFill="#ace2f5">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
                <TextField fx:id="titre" layoutX="238.0" layoutY="119.0" />
        
        <Label fx:id="descriptionLabel" layoutX="91.0" layoutY="166.0" text="Description :" textFill="#ace2f5">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
        <TextArea fx:id="area" layoutX="238.0" layoutY="153.0" prefHeight="34.0" prefWidth="149.0" />
        
       
        
        <Label fx:id="jourLabel" layoutX="92.0" layoutY="212.0" text="Frequence par jour :" textFill="#ace2f5">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
        <TextField fx:id="jour" layoutX="237.0" layoutY="208.0" />
        
        <Label fx:id="semaineLabel" layoutX="90.0" layoutY="268.0" text="Frequence par semaine :" textFill="#ace2f5">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
         <TextField fx:id="semaine" layoutX="237.0" layoutY="264.0" />
        
        <Label fx:id="titre1Column" layoutX="93.0" layoutY="310.0" text="Durée : " textFill="#ace2f5">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <TextField fx:id="duree" layoutX="237.0" layoutY="306.0" />
      
            <Label fx:id="myLabel211" layoutX="93.0" layoutY="340.0" text="Organe :" textFill="#ace2f5">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <ComboBox fx:id="organeComboBox" layoutX="238.0" layoutY="336.0" prefWidth="150.0" />
      
        <Button fx:id="Ajouter" layoutX="145.0" layoutY="384.0" mnemonicParsing="false" onAction="#ajoutSymptome" text="Ajouter" textFill="WHITE" />
        <Button fx:id="supprimer" layoutX="269.0" layoutY="384.0" mnemonicParsing="false" onAction="#deleteSymtome" text="Supprimer" textFill="WHITE" />
        <TableView fx:id="tableView" layoutX="460.0" layoutY="127.0" prefHeight="281.0" prefWidth="648.0">
            <columns>
                <TableColumn fx:id="titreColumn" prefWidth="63.0" text="Titre :" />
                <TableColumn fx:id="descriptionColmn" prefWidth="100.0" text="Description :" />
                <TableColumn fx:id="parjourColumn" prefWidth="166.0" text="Frequence par semaine :" />
                <TableColumn fx:id="parsemaineColumn" prefWidth="166.0" text="Frequence par semaine :" />
            <TableColumn fx:id="dureeColumn" prefWidth="81.0" text="Durée : " />
            <TableColumn fx:id="organeColumn1" prefWidth="81.0" text="Organe :" />
            </columns>
        </TableView>
      <TextField fx:id="searchField" layoutX="460.0" layoutY="76.0" prefHeight="37.0" prefWidth="648.0" promptText="Search" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backButtonClicked" text="Back" />

    </children>
</AnchorPane>
